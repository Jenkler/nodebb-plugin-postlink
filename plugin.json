{
	"id": "nodebb-plugin-postlink",
	"name": "NodeBB Postlink",
	"description": "A plugin that adds a prefix on postlinks.",
	"url": "https://github.com/Jenkler/nodebb-plugin-postlink",
	"library": "./library.js",
	"hooks": [
		{
			"hook": "filter:admin.header.build", "method": "addAdminNavigation"
		},
		{
			"hook": "filter:parse.post", "method": "parse", "priority": 9
		},
		{
			"hook": "static:app.load", "method": "init"
		}
	],
	"nbbpm": {
		"compatibility": "^0.6.0"
	},
	"templates": "./templates"
}
